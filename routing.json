{
  "routing": {
    "strategy": "rules",
    "settings": {
      "domainStrategy": "IPIfNonMatch",
      "domainMatcher": "mph",
      "rules": [
        {
          "comment": "拦截投放网页广告、弹窗广告、病毒木马、钓鱼欺诈性网站",
          "domain": [
            "ext:h1y.dat:rej",
            "geosite:category-ads-all"
          ],
          "type": "field",
          "outboundTag": "block"
        },
        {
          "comment": "拦截投放网页广告、弹窗广告、病毒木马、钓鱼欺诈性IP",
          "ip": [
            "geoip:rej"
          ],
          "type": "field",
          "outboundTag": "block"
        },
        {
          "type": "field",
          "ip": [
            "ext:geoip-only-cn-private.dat:private"
          ],
          "outboundTag": "block"
        },
        {
          "comment": "访问境外、GFW名单里的网站代理转发，使用vpn加速",
          "type": "field",
          "outboundTag": "proxy",
          "domain": [
            "geosite:geolocation-!cn",
            "ext:h1y.dat:gfw"
          ]
        },
        {
          "comment": "访问中国网站本地直连，不用vpn加速",
          "type": "field",
          "outboundTag": "direct",
          "domain": [
            "geosite:cn",
            "ext:h1y.dat:cn"
          ]
        },
        {
          "comment": "访问中国境内IP本地直连，不用vpn加速",
          "type": "field",
          "outboundTag": "direct",
          "ip": [
            "geoip:cn"
          ]
        }
      ]
    }
  }
}
